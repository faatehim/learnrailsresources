<%= form_for @python, html: { multipart: true } do |f| %>
  
 <div class="form">

  <div class="field">
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </div>

  <div class="field">
    <%= f.label :url %><br>
    <%= f.text_area :url%>
  </div>

  <div class="field">
    <%= f.label :shortdescription %><br>
    <%= f.text_area :shortdescription %>
  </div>

  <div class="field">
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </div>

  <div class="field">
    <%= f.label :tag %><br>
    <%= f.text_area :tag %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
</div>